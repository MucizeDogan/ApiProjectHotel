@model UpdateViewModel
@{
    ViewData["Title"] = "UpdateStaff";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<form method="post">
    <input type="hidden" asp-for="@Model.StaffID" />
    <label> Staff Name </label>
    <input type="text" asp-for="@Model.Name" class="form-control" />
    <br />
     <label>Staff Title </label>
    <input type="text" asp-for="@Model.JobTitle" class="form-control" />
    <br />
     <label> TwitterUrl </label>
    <input type="text" asp-for="@Model.TwitterUrl" class="form-control" />
    <br />
     <label> InstagramUrl </label>
    <input type="text" asp-for="@Model.InstagramUrl" class="form-control" />
    <br />
    <button class="btn btn-outline-success">Update</button>
</form>
