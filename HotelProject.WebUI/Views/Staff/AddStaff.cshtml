@model AddStaffViewModel
@{
    ViewData["Title"] = "AddStaff";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<form method="post">
    <label> Staff Name </label>
    <input type="text" asp-for="Name" class="form-control" />
    <br />
     <label>Staff Title </label>
    <input type="text" asp-for="JobTitle" class="form-control" />
    <br />
     <label> TwitterUrl </label>
    <input type="text" asp-for="TwitterUrl" class="form-control" />
    <br />
     <label> InstagramUrl </label>
    <input type="text" asp-for="InstagramUrl" class="form-control" />
    <br />
    <button class="btn btn-primary">Add</button>
</form>

