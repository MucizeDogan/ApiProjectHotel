@model List<ListBookingDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<link href="~/adminTheme/plugins/tables/css/datatable/dataTables.bootstrap4.min.css" rel="stylesheet">
<link href="css/style.css" rel="stylesheet">



<div class="container-fluid">
    <div class="row">
        @*<div class="col-12">*@
        <select name="DataTables_Table_0_length" aria-controls="DataTables_Table_0" class="form-control form-control-sm"><option value="10">10</option><option value="25">25</option><option value="50">50</option><option value="100">100</option></select>
        @*<div class="card">*@
        @*<div class="card-body">*@
        @*<h4 class="card-title">Data Table</h4>*@
        @*<div class="table-responsive">*@
        <table class="table table-striped table-bordered zero-configuration">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Name</th>
                    <th>Mail</th>
                    <th>Check In</th>
                    <th>Check Out</th>
                    <th>Adult Count</th>
                     <th>Adult Count</th>
                      <th>Adult Count</th>
                       <th>Special</th>
                        <th>Desc</th>
                         <th>Status</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {


                    <tr>
                        <td>@item.BookingID</td>
                      
                        <td>@item.Name</td>
                        <td>@item.Mail</td>
                        <td>@item.CheckIn</td>
                        <td>@item.CheckOut</td>
                        <td>@item.AdultCount</td>
                        <td>@item.ChildCount</td>
                        <td>@item.RoomCount</td>
                        <td>@item.SpecialRequest</td>
                        <td>@item.Description</td>
                        <td>@item.Status</td>
                        <td><a href="#" class="btn btn-dark">Details</a></td>
                        <td><a href="/Service/UpdateService/@item.BookingID" class="btn mb-1 btn-rounded btn-warning">Update</a></td>
                        <td><a href="/Service/DeleteService/@item.BookingID" class="btn btn-danger">Delete</a></td>
                    </tr>
                }
                <tr>
            </tbody>
        </table>

        @*</div>*@
        @*</div>*@
        @* </div>*@
        @*</div>*@
    </div>
</div>
<a href="/Service/AddService/" class="btn btn-primary">New Service Add</a>





<script src="~/adminTheme/plugins/common/common.min.js"></script>
<script src="js/custom.min.js"></script>
<script src="js/settings.js"></script>
<script src="js/gleek.js"></script>
<script src="js/styleSwitcher.js"></script>

<script src="~/adminTheme/plugins/tables/js/jquery.dataTables.min.js"></script>
<script src="~/adminTheme/plugins/tables/js/datatable/dataTables.bootstrap4.min.js"></script>
<script src="~/adminTheme/plugins/tables/js/datatable-init/datatable-basic.min.js"></script>